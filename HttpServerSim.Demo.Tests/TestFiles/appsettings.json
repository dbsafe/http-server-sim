{
  // This is the config file used when the app executes with workingDirectory set to this working directory
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information",
      "HttpServerSim": "Debug"
    }
  },
  "AllowedHosts": "*",
  "HttpServerSim": {
    "RulesPath": "rules.json",
    "Url": "http://localhost:8080",
    "ControlUrl": "http://localhost:8090",
    "LogControlRequestAndResponse": true,
    "LogRequestAndResponse": true
  }
}
